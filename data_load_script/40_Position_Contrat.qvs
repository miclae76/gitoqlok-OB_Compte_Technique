//    "D_BILAN",
    "ID_CONTRAT",
//    "CD_SYS_GESTION",
    "NUM_CONTRAT",
    "CD_SYS_GESTION" & "NUM_CONTRAT" AS _CLE_AGE,
    "NUM_CTR_FIRME",
	IF(year("D_EFFET")<year(D_BILAN), 'STOCK', 'NEW B') AS STOCK_OR_NEWB,
